<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="com.demo.demo.auto.wire.Address" name="add">
        <property name="street" value="Kashipuri"/>
        <property name="city" value="Indore"/>
    </bean>
    <bean class="com.demo.demo.auto.wire.Address" name="address">
        <property name="street" value="Kashipuri2"/>
        <property name="city" value="Indore2"/>
    </bean>


    <bean class="com.demo.demo.auto.wire.Emp" name="emp" autowire="byName"/>


</beans>
